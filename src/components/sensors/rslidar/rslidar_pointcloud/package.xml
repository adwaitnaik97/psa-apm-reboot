<package format ="2">

  <name>rslidar_pointcloud</name>
  <version>1.0.0</version>
  <description>
    Point cloud conversions for rslidar 3D LIDARs.
  </description>
  <maintainer email="tony.zhang@robosense.cn">Tony</maintainer>
  <author>Tony Zhang</author>
  <author>George Wang</author>
  <author>Piyush Khandelwal</author>
  <author>Jesse Vera</author>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>angles</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>pcl_conversions</build_depend>
  <build_depend>pcl_ros</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>roslib</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>rslidar_driver</build_depend>
  <build_depend>rslidar_msgs</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>

  <!-- these build dependencies are only needed for unit testing -->
  <build_depend>roslaunch</build_depend>
  <build_depend>rostest</build_depend>
  <build_depend>tf2_ros</build_depend>

  <exec_depend>angles</exec_depend>
  <exec_depend>nodelet</exec_depend>
  <exec_depend>pcl_ros</exec_depend>
  <exec_depend>pluginlib</exec_depend>
  <!-- <exec_depend>python-yaml</exec_depend> -->
  <exec_depend>roscpp</exec_depend>
  <exec_depend>roslib</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>tf</exec_depend>
  <exec_depend>rslidar_driver</exec_depend>
  <exec_depend>rslidar_msgs</exec_depend>
  <!-- <exec_depend>yaml-cpp</exec_depend> -->
  <exec_depend>dynamic_reconfigure</exec_depend>

  <export>
    <nodelet plugin="${prefix}/nodelets.xml"/>
  </export>
</package>
